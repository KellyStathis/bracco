<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">
      {{pluralize-word model.currentProviderCount "Provider" omitCount=true}}
      <span class="small pull-right">by year</span>
    </h3>
  </div>
  <div class="panel-body panel-chart">
    <h2 class="member graphs">{{#link-to "providers"}}{{format-number model.currentProviderCount}}{{/link-to}}</h2>
    <div class="graphs" id="chart-provider"></div>
  </div>
</div>
