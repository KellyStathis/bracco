{{#if (can "read index")}}
  {{application-header default=true}}

  <div class="container-fluid">
    <div class="content">
      {{index-header}}

      <div class="row">
        <div class="col-md-9">
          <div class="panel panel-transparent" id="content-member">
            <div class="panel-body">
              {{#each flashMessages.queue as |flash|}}
                {{flash-message flash=flash}}
              {{/each}}

              {{index-show}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
{{else}}
  {{application-header}}
  {{landing-page}}
{{/if}}
