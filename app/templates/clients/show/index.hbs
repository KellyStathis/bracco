<div class="row">
  <div class="col-md-9">
    <div class="panel panel-transparent" id="content-member">
      <div class="panel-body">
        {{#if (gt model.meta.prefixes 1)}}
          <div>{{model.name}} uses <span class="icon-info">{{pluralize-word model.meta.prefixes "prefix"}}</span>.</div>
        {{else}}
          <div>{{model.name}} uses <span class="icon-warning">only the test prefix 10.5072.</span>. Ask the provider {{model.provider.name}} to add prefixes.</div>
        {{/if}}
      </div>
    </div>
  </div>
</div>
